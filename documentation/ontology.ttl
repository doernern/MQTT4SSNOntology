xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#forwardsPublishPacket
:forwardsPublishPacket rdf:type owl:ObjectProperty ;
                       rdfs:domain :Broker ;
                       rdfs:range :PublishPacket ;
                       rdfs:comment "Indicates that an MQTT Broker forwards a PUBLISH packet to subscribers."@en ;
                       rdfs:label "forwards PUBLISH Packet"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasClient
:hasClient rdf:type owl:ObjectProperty ;
           owl:inverseOf :runsOnPlatform ;
           rdfs:domain <http://www.w3.org/ns/sosa/Platform> ;
           rdfs:range :Client ;
           rdfs:comment "Relates a SOSA platform to an MQTT Client running on it."@en ;
           rdfs:label "has MQTT Client"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasConnectedClient
:hasConnectedClient rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isConnectedToBroker ;
                    rdfs:domain :Broker ;
                    rdfs:range :Client ;
                    rdfs:comment "Links an MQTT Broker to an MQTT Client that is connected to it."@en ;
                    rdfs:label "has connected MQTT Client"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasEncodedFields
:hasEncodedFields rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isEncodedInPayload ;
                  rdfs:domain :PublishPayload ;
                  rdfs:range rdf:List ;
                  rdfs:comment "Links an MQTT Payload to an ordered RDF list naming fields encoded within the payload content."@en ;
                  rdfs:label "has encoded fields"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasFixedHeader
:hasFixedHeader rdf:type owl:ObjectProperty ;
                owl:inverseOf :isFixedHeaderOf ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain :ControlPacket ;
                rdfs:range :FixedHeader ;
                rdfs:comment "Links an MQTT Control Packet to its Fixed Header."@en ;
                rdfs:label "has Fixed Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasParticipant
:hasParticipant rdf:type owl:ObjectProperty ;
                owl:inverseOf :usesConnection ;
                rdfs:domain :NetworkConnection ;
                rdfs:range :NetworkParticipant ;
                rdfs:comment "Relates a network connection to its network participant (MQTT Client or MQTT Broker)."@en ;
                rdfs:label "has participant"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasPayload
:hasPayload rdf:type owl:ObjectProperty ;
            owl:equivalentProperty <https://www.w3.org/2019/wot/mqtt#hasPayload> ;
            owl:inverseOf :isPayloadOf ;
            rdf:type owl:FunctionalProperty ;
            rdfs:domain :ControlPacket ;
            rdfs:range :Payload ;
            rdfs:comment "Links an MQTT Control Packet to its MQTT Payload."@en ;
            rdfs:label "has MQTT Payload"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasPayloadSize
:hasPayloadSize rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :Payload ;
                rdfs:range <http://qudt.org/schema/qudt/QuantityValue> ;
                rdfs:comment "Specifies the size of an MQTT Payload as a QUDT quantity value (e.g., unit:Byte)."@en ;
                rdfs:label "has payload size"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasPublishFixedHeader
:hasPublishFixedHeader rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf :hasFixedHeader ;
                       owl:inverseOf :isPublishFixedHeaderOf ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain :PublishPacket ;
                       rdfs:range :PublishFixedHeader ;
                       rdfs:comment "Links a PUBLISH packet to its specific Fixed Header."@en ;
                       rdfs:label "has PUBLISH Fixed Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasPublishPayload
:hasPublishPayload rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :hasPayload ;
                   owl:inverseOf :isPublishPayloadOf ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :PublishPacket ;
                   rdfs:range :PublishPayload ;
                   rdfs:comment "Links a PUBLISH packet to its MQTT Payload."@en ;
                   rdfs:label "has PUBLISH Payload"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasPublishVariableHeader
:hasPublishVariableHeader rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :hasVariableHeader ;
                          owl:inverseOf :isPublishVariableHeaderOf ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain :PublishPacket ;
                          rdfs:range :PublishVariableHeader ;
                          rdfs:comment "Links a PUBLISH packet to its specific Variable Header."@en ;
                          rdfs:label "has PUBLISH Variable Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasSubject
:hasSubject rdf:type owl:ObjectProperty ;
            owl:inverseOf :isSubjectOf ;
            rdfs:domain :Topic ;
            rdfs:range :TopicSubject ;
            rdfs:comment "Links an MQTT Topic to the conceptual subject carried on that topic."@en ;
            rdfs:label "has topic subject"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasSubscriptionFixedHeader
:hasSubscriptionFixedHeader rdf:type owl:ObjectProperty ;
                            rdfs:subPropertyOf :hasFixedHeader ;
                            owl:inverseOf :isSubscriptionFixedHeaderOf ;
                            rdf:type owl:FunctionalProperty ;
                            rdfs:domain :SubscribePacket ;
                            rdfs:range :SubscribeFixedHeader ;
                            rdfs:comment "Links a SUBSCRIBE packet to its specific Fixed Header."@en ;
                            rdfs:label "has SUBSCRIBE Fixed Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasSubscriptionPayload
:hasSubscriptionPayload rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :hasPayload ;
                        owl:inverseOf :isSubscriptionPayloadOf ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain :SubscribePacket ;
                        rdfs:range :SubscriptionPayload ;
                        rdfs:comment "Links a SUBSCRIBE packet to its MQTT Payload."@en ;
                        rdfs:label "has SUBSCRIBE Payload"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasSubscriptionVariableHeader
:hasSubscriptionVariableHeader rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :hasVariableHeader ;
                               owl:inverseOf :isSubscriptionVariableHeaderOf ;
                               rdf:type owl:FunctionalProperty ;
                               rdfs:domain :SubscribePacket ;
                               rdfs:range :SubscribeVariableHeader ;
                               rdfs:comment "Links a SUBSCRIBE packet to its specific Variable Header."@en ;
                               rdfs:label "has SUBSCRIBE Variable Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasSubscriptions
:hasSubscriptions rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isSubscriptionOf ;
                  rdfs:domain :SubscriptionPayload ;
                  rdfs:range :SubscriptionEntry ;
                  rdfs:comment "Links a SUBSCRIBE Payload to one or more MQTT Subscription Entries."@en ;
                  rdfs:label "has MQTT Subscription Entries"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasTopicFilter
:hasTopicFilter rdf:type owl:ObjectProperty ;
                owl:equivalentProperty <https://www.w3.org/2019/wot/mqtt#hasTopicFilters> ;
                owl:inverseOf :isTopicFilterOf ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( :SubscriptionEntry
                                            :UnsubscriptionPayload
                                          )
                            ] ;
                rdfs:range :TopicFilter ;
                rdfs:comment "Relates a Subscription Entry or an UNSUBSCRIBE payload to an MQTT Topic Filter."@en ;
                rdfs:label "has MQTT Topic Filter"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasTopicName
:hasTopicName rdf:type owl:ObjectProperty ;
              owl:equivalentProperty <https://www.w3.org/2019/wot/mqtt#hasTopicName> ;
              owl:inverseOf :isTopicNameOf ;
              rdf:type owl:FunctionalProperty ;
              rdfs:domain :PublishVariableHeader ;
              rdfs:range :Topic ;
              rdfs:comment "Links a PUBLISH Variable Header to the MQTT Topic to which the message is published."@en ;
              rdfs:label "has MQTT Topic (name)"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasUnsubscriptionFixedHeader
:hasUnsubscriptionFixedHeader rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf :hasFixedHeader ;
                              owl:inverseOf :isUnsubscriptionFixedHeaderOf ;
                              rdf:type owl:FunctionalProperty ;
                              rdfs:domain :UnsubscribePacket ;
                              rdfs:range :UnsubscribeFixedHeader ;
                              rdfs:comment "Links an UNSUBSCRIBE packet to its specific Fixed Header."@en ;
                              rdfs:label "has UNSUBSCRIBE Fixed Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasUnsubscriptionPayload
:hasUnsubscriptionPayload rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf :hasPayload ;
                          owl:inverseOf :isUnsubscriptionPayloadOf ;
                          rdf:type owl:FunctionalProperty ;
                          rdfs:domain :UnsubscribePacket ;
                          rdfs:range :UnsubscriptionPayload ;
                          rdfs:comment "Links an UNSUBSCRIBE packet to its MQTT Payload."@en ;
                          rdfs:label "has UNSUBSCRIBE Payload"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasUnsubscriptionVariableHeader
:hasUnsubscriptionVariableHeader rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf :hasVariableHeader ;
                                 owl:inverseOf :isUnsubscriptionVariableHeaderOf ;
                                 rdf:type owl:FunctionalProperty ;
                                 rdfs:domain :UnsubscribePacket ;
                                 rdfs:range :UnsubscribeVariableHeader ;
                                 rdfs:comment "Links an UNSUBSCRIBE packet to its specific Variable Header."@en ;
                                 rdfs:label "has UNSUBSCRIBE Variable Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hasVariableHeader
:hasVariableHeader rdf:type owl:ObjectProperty ;
                   owl:inverseOf :isVariableHeaderOf ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain :ControlPacket ;
                   rdfs:range :VariableHeader ;
                   rdfs:comment "Links an MQTT Control Packet to its Variable Header."@en ;
                   rdfs:label "has Variable Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#hosts
:hosts rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf <http://www.w3.org/ns/sosa/hosts> ;
       owl:inverseOf :isHostedBy ;
       rdfs:domain :Client ;
       rdfs:range <http://www.w3.org/ns/sosa/System> ;
       rdfs:comment "Links an MQTT Client (as a SOSA platform) to SOSA systems (sensors/actuators) it hosts."@en ;
       rdfs:label "hosts"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isAcceptedBy
:isAcceptedBy rdf:type owl:ObjectProperty ;
              rdfs:domain :NetworkConnection ;
              rdfs:range :Broker ;
              rdfs:comment "Indicates the MQTT Broker that accepted the client's connection request."@en ;
              rdfs:label "is accepted by"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isConnectedToBroker
:isConnectedToBroker rdf:type owl:ObjectProperty ;
                     rdfs:domain :Client ;
                     rdfs:range :Broker ;
                     rdfs:comment "Links an MQTT Client to the MQTT Broker it is currently connected to."@en ;
                     rdfs:label "is connected to MQTT Broker"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isEncodedInPayload
:isEncodedInPayload rdf:type owl:ObjectProperty ;
                    rdfs:domain rdf:List ;
                    rdfs:range :PublishPayload ;
                    rdfs:comment "Links an RDF list of field names to the MQTT Payload that contains them."@en ;
                    rdfs:label "is encoded in MQTT Payload"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isFixedHeaderOf
:isFixedHeaderOf rdf:type owl:ObjectProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain :FixedHeader ;
                 rdfs:range :ControlPacket ;
                 rdfs:comment "Links a Fixed Header to the MQTT Control Packet containing it."@en ;
                 rdfs:label "is Fixed Header of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isHostedBy
:isHostedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf <http://www.w3.org/ns/sosa/isHostedBy> ;
            rdfs:domain <http://www.w3.org/ns/sosa/System> ;
            rdfs:range :Client ;
            rdfs:comment "Links a SOSA system to the hosting MQTT Client."@en ;
            rdfs:label "is hosted by"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isInitiatedBy
:isInitiatedBy rdf:type owl:ObjectProperty ;
               rdfs:domain :NetworkConnection ;
               rdfs:range :Client ;
               rdfs:comment "Indicates the MQTT Client that initiated the connection to the MQTT Broker."@en ;
               rdfs:label "is initiated by"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isMatchedByFilter
:isMatchedByFilter rdf:type owl:ObjectProperty ;
                   owl:inverseOf :matchesTopic ;
                   rdfs:domain :Topic ;
                   rdfs:range :TopicFilter ;
                   rdfs:comment "Indicates that an MQTT Topic is matched by a given MQTT Topic Filter."@en ;
                   rdfs:label "is matched by MQTT Topic Filter"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isPayloadOf
:isPayloadOf rdf:type owl:ObjectProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :Payload ;
             rdfs:range :ControlPacket ;
             rdfs:comment "Links an MQTT Payload to the MQTT Control Packet containing it."@en ;
             rdfs:label "is MQTT Payload of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isPublishFixedHeaderOf
:isPublishFixedHeaderOf rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf :isFixedHeaderOf ;
                        rdf:type owl:FunctionalProperty ;
                        rdfs:domain :PublishFixedHeader ;
                        rdfs:range :PublishPacket ;
                        rdfs:comment "Links a PUBLISH Fixed Header to its MQTT Control Packet."@en ;
                        rdfs:label "is PUBLISH Fixed Header of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isPublishPayloadOf
:isPublishPayloadOf rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :isPayloadOf ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain :PublishPayload ;
                    rdfs:range :PublishPacket ;
                    rdfs:comment "Links a PUBLISH Payload to its MQTT Control Packet."@en ;
                    rdfs:label "is PUBLISH Payload of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isPublishVariableHeaderOf
:isPublishVariableHeaderOf rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :isVariableHeaderOf ;
                           rdf:type owl:FunctionalProperty ;
                           rdfs:domain :PublishVariableHeader ;
                           rdfs:range :PublishPacket ;
                           rdfs:comment "Links a PUBLISH Variable Header to its MQTT Control Packet."@en ;
                           rdfs:label "is PUBLISH Variable Header of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isSentBy
:isSentBy rdf:type owl:ObjectProperty ;
          owl:inverseOf :sendsControlPacket ;
          rdfs:domain :ControlPacket ;
          rdfs:range :Client ;
          rdfs:comment "Relates an MQTT Control Packet to the MQTT Client that sent it."@en ;
          rdfs:label "is sent by MQTT Client"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isSubjectOf
:isSubjectOf rdf:type owl:ObjectProperty ;
             rdfs:domain :TopicSubject ;
             rdfs:range :Topic ;
             rdfs:comment "Links a topic subject (e.g., Observation, Actuation) to the MQTT Topic that carries it."@en ;
             rdfs:label "is topic subject of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isSubscriptionFixedHeaderOf
:isSubscriptionFixedHeaderOf rdf:type owl:ObjectProperty ;
                             rdfs:subPropertyOf :isFixedHeaderOf ;
                             rdf:type owl:FunctionalProperty ;
                             rdfs:domain :SubscribeFixedHeader ;
                             rdfs:range :SubscribePacket ;
                             rdfs:comment "Links a SUBSCRIBE Fixed Header to its MQTT Control Packet."@en ;
                             rdfs:label "is SUBSCRIBE Fixed Header of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isSubscriptionOf
:isSubscriptionOf rdf:type owl:ObjectProperty ;
                  rdfs:domain :SubscriptionEntry ;
                  rdfs:range :SubscriptionPayload ;
                  rdfs:comment "Links an MQTT Subscription Entry to its SUBSCRIBE Payload."@en ;
                  rdfs:label "is MQTT Subscription Entry of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isSubscriptionPayloadOf
:isSubscriptionPayloadOf rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf :isPayloadOf ;
                         rdf:type owl:FunctionalProperty ;
                         rdfs:domain :SubscriptionPayload ;
                         rdfs:range :SubscribePacket ;
                         rdfs:comment "Links a SUBSCRIBE Payload to its MQTT Control Packet."@en ;
                         rdfs:label "is SUBSCRIBE Payload of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isSubscriptionVariableHeaderOf
:isSubscriptionVariableHeaderOf rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf :isVariableHeaderOf ;
                                rdf:type owl:FunctionalProperty ;
                                rdfs:domain :SubscribeVariableHeader ;
                                rdfs:range :SubscribePacket ;
                                rdfs:comment "Links a SUBSCRIBE Variable Header to its MQTT Control Packet."@en ;
                                rdfs:label "is SUBSCRIBE Variable Header of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isTopicFilterOf
:isTopicFilterOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :TopicFilter ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( :SubscriptionEntry
                                            :UnsubscriptionPayload
                                          )
                            ] ;
                 rdfs:comment "Relates an MQTT Topic Filter to a Subscription Entry or an UNSUBSCRIBE payload."@en ;
                 rdfs:label "is MQTT Topic Filter of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isTopicNameOf
:isTopicNameOf rdf:type owl:ObjectProperty ;
               rdfs:domain :Topic ;
               rdfs:range :PublishVariableHeader ;
               rdfs:comment "Links an MQTT Topic to which the message is published to a PUBLISH Variable Header."@en ;
               rdfs:label "is MQTT Topic (name) of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isUnsubscriptionFixedHeaderOf
:isUnsubscriptionFixedHeaderOf rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf :isFixedHeaderOf ;
                               rdf:type owl:FunctionalProperty ;
                               rdfs:domain :UnsubscribeFixedHeader ;
                               rdfs:range :UnsubscribePacket ;
                               rdfs:comment "Links an UNSUBSCRIBE Fixed Header to its MQTT Control Packet."@en ;
                               rdfs:label "is UNSUBSCRIBE Fixed Header of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isUnsubscriptionPayloadOf
:isUnsubscriptionPayloadOf rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf :isPayloadOf ;
                           rdf:type owl:FunctionalProperty ;
                           rdfs:domain :UnsubscriptionPayload ;
                           rdfs:range :UnsubscribePacket ;
                           rdfs:comment "Links an UNSUBSCRIBE Payload to its MQTT Control Packet."@en ;
                           rdfs:label "is UNSUBSCRIBE Payload of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isUnsubscriptionVariableHeaderOf
:isUnsubscriptionVariableHeaderOf rdf:type owl:ObjectProperty ;
                                  rdfs:subPropertyOf :isVariableHeaderOf ;
                                  rdf:type owl:FunctionalProperty ;
                                  rdfs:domain :UnsubscribeVariableHeader ;
                                  rdfs:range :UnsubscribePacket ;
                                  rdfs:comment "Links an UNSUBSCRIBE Variable Header to its MQTT Control Packet."@en ;
                                  rdfs:label "is UNSUBSCRIBE Variable Header of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#isVariableHeaderOf
:isVariableHeaderOf rdf:type owl:ObjectProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain :VariableHeader ;
                    rdfs:range :ControlPacket ;
                    rdfs:comment "Links a Variable Header to the MQTT Control Packet containing it."@en ;
                    rdfs:label "is Variable Header of"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#listensToTopic
:listensToTopic rdf:type owl:ObjectProperty ;
                rdfs:domain <http://www.w3.org/ns/sosa/Actuator> ;
                rdfs:range :Topic ;
                rdfs:comment "Links an actuator to the MQTT Topic from which it receives actuation commands."@en ;
                rdfs:label "listens to MQTT Topic"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#matchesTopic
:matchesTopic rdf:type owl:ObjectProperty ;
              rdfs:domain :TopicFilter ;
              rdfs:range :Topic ;
              rdfs:comment "Indicates that an MQTT Topic Filter matches a particular MQTT Topic."@en ;
              rdfs:label "matches MQTT Topic"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#observesTopic
:observesTopic rdf:type owl:ObjectProperty ;
