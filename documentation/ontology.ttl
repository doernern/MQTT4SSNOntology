###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#subscribesPacket
:subscribesPacket rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :sendsControlPacket ;
                  rdfs:domain :Client ;
                  rdfs:range :SubscribePacket ;
                  rdfs:comment "Specializes mqtt:sendsControlPacket for SUBSCRIBE packets."@en ;
                  rdfs:label "sends SUBSCRIBE Packet"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN#unsubscribesPacket
:unsubscribesPacket rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf :sendsControlPacket ;
                    rdfs:domain :Client ;
                    rdfs:range :UnsubscribePacket ;
                    rdfs:comment "Specializes mqtt:sendsControlPacket for UNSUBSCRIBE packets."@en ;
                    rdfs:label "sends UNSUBSCRIBE Packet"@en .


