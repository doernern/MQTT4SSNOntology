###  https://www.w3id.org/MQTT4SSN-Ontology#hasQosLevel
:hasQosLevel rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :PublishFixedHeader
                                         :SubscriptionEntry
                                       )
                         ] ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:onDatatype xsd:integer ;
                          owl:withRestrictions ( [ xsd:minInclusive 0
                                                 ]
                                                 [ xsd:maxInclusive 2
                                                 ]
                                               )
                        ] ;
             rdfs:comment "Not mapped to mqv:hasQoSFlag because MQV models QoS as an object property, while mqtt:hasQosLevel is a datatype property."@en ,
                          "The MQTT Quality of Service level (0 = at most once, 1 = at least once, 2 = exactly once)."@en ;
             rdfs:label "QoS level"@en .


###  https://www.w3id.org/MQTT4SSN-Ontology#isDUPFlag
:isDUPFlag rdf:type owl:DatatypeProperty ;
           rdfs:domain :PublishFixedHeader ;
           rdfs:range xsd:boolean ;
           rdfs:comment "True if this is a duplicate delivery of an earlier PUBLISH message."@en ;
           rdfs:label "DUP flag"@en .


###  https://www.w3id.org/MQTT4SSN-Ontology#isRetainedFlag
:isRetainedFlag rdf:type owl:DatatypeProperty ;
                rdfs:domain :PublishFixedHeader ;
                rdfs:range xsd:boolean ;
                rdfs:comment "True if the message should be retained on the MQTT Broker."@en ;
                rdfs:label "RETAIN flag"@en .


###  https://www.w3id.org/MQTT4SSN-Ontology#usesTLS
:usesTLS rdf:type owl:DatatypeProperty ,
                  owl:FunctionalProperty ;
         rdfs:domain :NetworkConnection ;
         rdfs:range xsd:boolean ;
         rdfs:comment "True if the Network Connection is protected via TLS, otherwise false."@en ;
         rdfs:label "uses TLS"@en .


