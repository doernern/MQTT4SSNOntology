                                                                      foaf:mbox <mailto:maleshkm@hsu-hh.de> ;
                                                                      foaf:name "Maria Maleshkova"
                                                                    ] ;
                                                        dct:license <https://creativecommons.org/licenses/by-nc-sa/4.0/> ;
