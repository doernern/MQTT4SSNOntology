###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#SubscribePacket
:SubscribePacket rdf:type owl:Class ;
                 owl:equivalentClass mqv:SUBSCRIBE ;
                 rdfs:subClassOf :ControlPacket ;
                 rdfs:comment "An MQTT Control Packet used by an MQTT Client to subscribe to one or more MQTT Topic Filters."@en ;
                 rdfs:label "MQTT SUBSCRIBE Packet"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#SubscribeVariableHeader
:SubscribeVariableHeader rdf:type owl:Class ;
                         rdfs:subClassOf :VariableHeader ;
                         rdfs:comment "The Variable Header of a SUBSCRIBE packet; typically includes the packet identifier."@en ;
                         rdfs:label "MQTT SUBSCRIBE Variable Header"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#SubscriptionEntry
:SubscriptionEntry rdf:type owl:Class ;
                   rdfs:comment "A single requested subscription consisting of an MQTT Topic Filter and an associated QoS level."@en ;
                   rdfs:label "MQTT Subscription Entry"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#SubscriptionPayload
:SubscriptionPayload rdf:type owl:Class ;
                     rdfs:subClassOf :Payload ;
                     rdfs:comment "The payload of a SUBSCRIBE packet, listing MQTT Subscription Entries."@en ;
                     rdfs:label "MQTT SUBSCRIBE Payload"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#Topic
:Topic rdf:type owl:Class ;
       owl:equivalentClass mqv:TopicName ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :hasActObsSubject ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass [ rdf:type owl:Class ;
                                       owl:unionOf ( sosa:Actuation
                                                     sosa:ActuationCollection
                                                     sosa:Observation
                                                     sosa:ObservationCollection
                                                   )
                                     ]
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasFoISubject ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasPropertySubject ;
                         owl:maxCardinality "1"^^xsd:nonNegativeInteger
                       ] ;
       rdfs:comment "A hierarchical MQTT Topic to which MQTT Clients publish and/or subscribe."@en ;
       rdfs:label "MQTT Topic"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#TopicFilter
:TopicFilter rdf:type owl:Class ;
             owl:equivalentClass mqv:TopicFilter ;
             rdfs:comment "A filter expression (e.g., with wildcards) used in SUBSCRIBE/UNSUBSCRIBE to match MQTT Topics."@en ;
             rdfs:label "MQTT Topic Filter"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#TopicSubject
:TopicSubject rdf:type owl:Class ;
