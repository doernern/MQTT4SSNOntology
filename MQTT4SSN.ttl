###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#forwardsPublishPacket
:forwardsPublishPacket rdf:type owl:ObjectProperty ;
                       rdfs:domain :Broker ;
                       rdfs:range :PublishPacket ;
                       rdfs:comment "Indicates that an MQTT Broker forwards a PUBLISH packet to subscribers."@en ;
                       rdfs:label "forwards PUBLISH Packet"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#hasActObsSubject
:hasActObsSubject rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :hasSubject ;
                  owl:inverseOf :isActObsSubjectOf ;
                  rdfs:domain :Topic ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( sosa:Actuation
                                             sosa:ActuationCollection
                                             sosa:Observation
                                             sosa:ObservationCollection
                                           )
                             ] ;
                  rdfs:comment "Links an MQTT Topic to the subject (Actuation, Actuation collection, Observation or Observation Collection)."@en ;
                  rdfs:label "has topic subject (Actuation, Actuation collection, Observation or Observation Collection)"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#hasClient
:hasClient rdf:type owl:ObjectProperty ;
           owl:inverseOf :runsOnPlatform ;
           rdfs:domain sosa:Platform ;
           rdfs:range :Client ;
           rdfs:comment "Relates a SOSA platform to an MQTT Client running on it."@en ;
           rdfs:label "has MQTT Client"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#hasConnectedClient
:hasConnectedClient rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isConnectedToBroker ;
                    rdfs:domain :Broker ;
                    rdfs:range :Client ;
                    rdfs:comment "Links an MQTT Broker to an MQTT Client that is connected to it."@en ;
                    rdfs:label "has connected MQTT Client"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#hasEncodedFields
:hasEncodedFields rdf:type owl:ObjectProperty ;
                  owl:inverseOf :isEncodedInPayload ;
                  rdfs:domain :PublishPayload ;
                  rdfs:range rdf:List ;
                  rdfs:comment "Links an MQTT Payload to an ordered RDF list naming fields encoded within the payload content."@en ;
                  rdfs:label "has encoded fields"@en .


###  https://doernern.github.io/MQTT4SSNOntology/MQTT4SSN.owl#hasFixedHeader
