    <owl:Ontology rdf:about="https://doernern.github.io/MQTT4SSNOntology/documentation/index-en.html">
        <owl:versionIRI rdf:resource="https://doernern.github.io/MQTT4SSNOntology/documentation/index-en.html"/>
